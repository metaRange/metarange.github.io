                            conf.inputs,
                            conf.outputs,
                            x,
                            y,
                            conf.className,
                            conf.data,
                            conf.html
                        );
                    }
                }
            });

            // Update code panel when nodes or connections change
            function updateCodePanel() {
                const nodes = Object.values(
                    editor.drawflow.drawflow[editor.module].data,
                );

                let code = `library(metaRange)\nlibrary(terra)\nset_verbosity(2)\n\n`;

                // If we have an environment node, we need to create the environment setup code
                const envNodes = nodes.filter(
                    (node) => node.class === "env-node",
                );
                let uniqueEnv,
                    envNames, 
                    envNamesLabel;
                if (envNodes.length > 0) {
                    code += `#### Setup of the environment: #### (replace this with your own data)\n`;

                    uniqueEnv = [...new Set(envNodes.map((node) => node.name))];
                    envNames = uniqueEnv
                        .join(", ")
                        .toLowerCase();
                    envNamesLabel = uniqueEnv
                        .join("', '")
                        .toLowerCase();

                    if (envNodes.some((node) => node.name === "Temperature")) {
                        code += `temperature <- rast(volcano + 150)\n`;
                    }
                    if (envNodes.some((node) => node.name === "Precipitation")) {
                        code += `precipitation <- rast(volcano * 4)\n`;
                    }
                    if (envNodes.some((node) => node.name === "Resource")) {
                        code += `resource <- rast((volcano - min(volcano)) / (max(volcano) -min(volcano)))\n`;
                    }

                } else {
                    envNames = "spatial_grid";
                    envNamesLabel = "spatial_grid";
                    code += `spatial_grid <- rast(volcano)\n`;
                }
                code += `landscape <- sds(${envNames})\n`;
                code += `names(landscape) <- c('${envNamesLabel}')\n\n`;
                code += `#### Create the simulation object\n`;
                code += `sim <- create_simulation(landscape)\n\n`;


                
                const startNodes = nodes.filter(
                    (node) => node.class === "species-node",
                );
                let speciesNames = startNodes
                    .map((node) => node.data?.vsSpeciesName);
                // startNodes
                if (startNodes.length > 0) {

                    code += `#### Add Species: ####\n`;
                    speciesNames = speciesNames
                        .map((name) => name)
                        .join("', '");
                    code += `sim$add_species(c('${speciesNames}'))\n\n`;

                    code += `#### Add traits: ####\n`;

                    startNodes.forEach((node) => {

                        if (node.outputs.output_1) {
                            for (const output of node.outputs.output_1.connections) {
                                let id = output.node;
                                let traitValue = editor.container.querySelectorAll(
                                    "#node-" + id + ' input[type="number"]',
                                );

                                traitValue.forEach((input) => {
                                    if (input.name === "dispersal_distance") {
                                        
                                        code += `sim$add_traits(\n`;
                                        code += `    "${node.data?.vsSpeciesName}",\n`;
                                        code += `    population_level = FALSE,\n`;
                                        code += `    dispersal_kernel = calculate_dispersal_kernel(\n`;
                                        code += `        max_dispersal_dist = ${input.value * 2},\n`;
                                        code += `        kfun = negative_exponential_function,\n`;
                                        code += `        mean_dispersal_dist = ${input.value}\n`;
                                        code += `    )\n`;
                                        code += `)\n`;

                                    } else {
                                        code += `sim$add_traits("${node.data?.vsSpeciesName || node.name}", ${input.name} =  ${input.value})\n`;
                                    }
                                    if (input.name === "reproduction_rate" || input.name === "carrying_capacity") {
                                        code += `sim$add_traits("${node.data?.vsSpeciesName || node.name}", initial_${input.name} =  ${input.value})\n`;
                                    }
                                });

                            }
                        }
                    });

                    const process_nodes = nodes.filter(
                        (node) => node.class.includes("proc-node")
                    );
                    if (process_nodes.length > 0) {
                        code += `\n#### Add processes: ####\n`;
                        process_nodes.forEach((node) => {


                            if (!node.data.vsSpeciesName || !node.data.upstreamConnected) {
                                return;
                            }
                            let abundance_input, 
                                reproduction_rate_input,
                                carrying_capacity_input,
                                dispersal_distance_input,
                                weights_input;
                            if (node.name === "Reproduction" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0] &&
                                node.inputs.input_3?.connections[0]
                            ) {
                                abundance_input = node.inputs.input_1?.connections[0] ? "self$traits$abundance" : "node_not_connected";
                                reproduction_rate_input = node.inputs.input_2?.connections[0] ? "self$traits$reproduction_rate" : "node_not_connected";
                                carrying_capacity_input = node.inputs.input_3?.connections[0] ? "self$traits$carrying_capacity" : "node_not_connected";

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "reproduction",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        self$traits$abundance <-\n`;
                                code +=  `            ricker_reproduction_model(\n`;
                                code +=  `                abundance = ${abundance_input},\n`;
                                code +=  `                reproduction_rate = ${reproduction_rate_input},\n`;
                                code +=  `                carrying_capacity = ${carrying_capacity_input}\n`;
                                code +=  `            )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;
                            }

                            if (node.name === "Mortality" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0]
                            ) {
                                abundance_input = node.inputs.input_1?.connections[0] ? "self$traits$abundance" : "node_not_connected";
                                let survival_rate_input = node.inputs.input_2?.connections[0] ? "self$traits$survival_rate" : "node_not_connected";

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "mortality",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        self$traits$abundance <-\n`;
                                code +=  `            matrix(rpois(\n`;
                                code +=  `                length(${abundance_input}),\n`;
                                code +=  `                ${abundance_input} * ${survival_rate_input}\n`;
                                code +=  `            ), nrow = nrow(self$traits$abundance), ncol = ncol(self$traits$abundance))\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;
                            }

                            if (node.name === "Dispersal" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0]
                            ) {
                                abundance_input = node.inputs.input_1?.connections[0] ? "self$traits$abundance" : "node_not_connected";
                                dispersal_distance_input = node.inputs.input_2?.connections[0] ? "self$traits$dispersal_kernel" : "node_not_connected";
                                let continueFunction = node.inputs.input_3?.connections[0] ? "," : "";
                                weights_input = node.inputs.input_3?.connections[0] ? 'self$traits$suitability' : null;

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "dispersal_process",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        self$traits$abundance <-\n`;
                                code +=  `            dispersal(\n`;
                                code +=  `               abundance = ${abundance_input},\n`;
                                code +=  `               dispersal_kernel = ${dispersal_distance_input}${continueFunction}\n`;
                                if (weights_input) {code +=  `               weights = ${weights_input}\n`;}
                                code +=  `            )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;
                            }

                            if (node.name === "Calculate Suitability" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0] &&
                                node.inputs.input_3?.connections[0]
                            ) {
                                // TODO
                                // could be removed by data attrib
                                const env_type = editor.container.querySelector("#node-" + node.id + ' .input_3').dataset.env_type;
                                const env_pref_type = editor.container.querySelector("#node-" + node.id + ' .input_2').dataset.env_type;

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "calculate_suitability",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        self$traits$suitability <-\n`;
                                code +=  `            calculate_suitability(\n`;
                                code +=  `                vmax = self$traits$max_${env_pref_type},\n`;
                                code +=  `                vopt = self$traits$optim_${env_pref_type},\n`;
                                code +=  `                vmin = self$traits$min_${env_pref_type},\n`;
                                code +=  `                venv = self$sim$environment$current$${env_type}\n`;
                                code +=  `            )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;

                            }

                            if (node.name === "Metabolic Scaling" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0] &&
                                node.inputs.input_3?.connections[0]
                            ) {
                                // TODO
                                // could be removed by data attrib
                                const mte_type = editor.container.querySelector("#node-" + node.id + ' .input_1').dataset.vsInputClasses;
                                let scaling_exponent, E_val, ftype;

                                if (mte_type === "reproduction_rate") {
                                    scaling_exponent = "-1/4";
                                    E_val = "-0.65";
                                    ftype = "max("
                                } else if (mte_type === "carrying_capacity") {
                                    scaling_exponent = "-3/4";
                                    E_val = "0.65";                                    
                                    ftype = "min("
                                }

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "metabolic_scaling_${mte_type}",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        normalization_constant <- calculate_normalization_constant(\n`;
                                code +=  `            parameter_value = mean(self$traits$initial_${mte_type}, na.rm=TRUE),\n`;
                                code +=  `            scaling_exponent = ${scaling_exponent},\n`;
                                code +=  `            mass = mean(self$traits$mass, na.rm = TRUE),\n`;
                                code +=  `            reference_temperature = ${ftype}self$sim$environment$current[["temperature"]], na.rm=TRUE),\n`;
                                code +=  `            E = ${E_val}\n`;
                                code +=  `        )\n`;
                                code +=  `        self$traits[["${mte_type}"]] <- metabolic_scaling(\n`;
                                code +=  `            normalization_constant = normalization_constant,\n`;
                                code +=  `            scaling_exponent = ${scaling_exponent},\n`;
                                code +=  `            mass = self$traits$mass,\n`;
                                code +=  `            temperature = self$sim$environment$current[["temperature"]],\n`;
                                code +=  `            E = ${E_val}\n`;
                                code +=  `        )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;

                            }

                            if (node.name === "Save" &&
                                node.inputs.input_1?.connections[0]
                            ) {
                                const save_node = editor.getNodeFromId(node.inputs.input_1.connections[0].node);
                                // TODO
                                // could be removed by data attrib
                                const trait_name = editor.container.querySelector("#node-" + node.id + ' .input_1').dataset.vsInputClasses;
                                let path = editor.container.querySelector("#node-" + node.id + ' input[type="text"]').value;
                                path = path !== "" ? path : "getwd()";

                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "saving_${save_node.data?.vsSpeciesName}_${trait_name}",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        save_species(\n`;
                                code +=  `            x = sim$${save_node.data?.vsSpeciesName},\n`;
                                code +=  `            traits = "${trait_name}",\n`;
                                code +=  `            prefix =  paste0(self$sim$get_current_time_step(), "-"),\n`;
                                code +=  `            path = ${path}\n`;
                                code +=  `        )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;

                            }


                            if (node.name === "Plot" &&
                                node.inputs.input_1?.connections[0]
                            ) {
                                // TODO
                                // could be removed by data attrib
                                const plot_node = editor.getNodeFromId(node.inputs.input_1.connections[0].node);
                                const trait_name = editor.container.querySelector("#node-" + node.id + ' .input_1').dataset.vsInputClasses;
                                code +=  `sim$add_process(\n`;
                                code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                code +=  `    process_name = "plotting_${plot_node.data?.vsSpeciesName}_${trait_name}",\n`;
                                code +=  `    process_fun = function() {\n`;
                                code +=  `        plot_cols <- hcl.colors(100, "Purple-Yellow", rev = TRUE)\n`;
                                code +=  `        plot(\n`;
                                code +=  `            sim$${plot_node.data?.vsSpeciesName},\n`;
                                code +=  `            trait = "${trait_name}",\n`;
                                code +=  `            main = "${plot_node.data?.vsSpeciesName} : ${trait_name}",\n`;
                                code +=  `            col = plot_cols\n`;
                                code +=  `        )\n`;
                                code +=  `    },\n`;
                                code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                code +=  `)\n`;

                            }

                            if (node.name === "Multiply With" &&
                                node.inputs.input_1?.connections[0] &&
                                node.inputs.input_2?.connections[0]
                            ) {
                                    const input_one = editor.container.querySelector("#node-" + node.id + ' .input_1').dataset;
                                    const input_two = editor.container.querySelector("#node-" + node.id + ' .input_2').dataset;
                                    const ioNameOne = input_one.vsInputClasses.replace(" ", "_");
                                    const ioNameTwo = input_two.vsInputClasses.replace(" ", "_");
                                    
                                    code +=  `sim$add_process(\n`;
                                    code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                                    code +=  `    process_name = "multiply_${ioNameOne}_with_${ioNameTwo}",\n`;
                                    code +=  `    process_fun = function() {\n`;
                                    if (input_two.vsSpeciesName === input_one.vsSpeciesName && input_two.env_type === undefined) {
                                    code +=  `        self$traits$${ioNameOne} <-\n`;
                                    code +=  `            self$traits$${ioNameOne} *\n`;
                                    code +=  `            self$traits$${ioNameTwo}\n`; 
                                    } else if (input_two.vsSpeciesName !== input_one.vsSpeciesName && input_two.env_type === undefined) {
                                    code +=  `        self$traits$${ioNameOne} <-\n`;
                                    code +=  `            self$traits$${ioNameOne} *\n`;
                                    code +=  `            self$sim$${input_two.vsSpeciesName}$traits$${ioNameTwo}\n`; 
                                    } else if (input_two.env_type !== undefined) {
                                    code +=  `        self$traits$${ioNameOne} <-\n`;
                                    code +=  `            self$traits$${ioNameOne} *\n`;
                                    code +=  `            self$sim$environment$current$${input_two.env_type}\n`; 
                                    }
                                    code +=  `    },\n`;
                                    code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                                    code +=  `)\n`;

                            }

                            
                            // if (node.name === "Scale 0-1" &&
                            //     node.inputs.input_1?.connections[0]
                            // ) {
                            //     const input_one = editor.container.querySelector("#node-" + node.id + ' .input_1').dataset;
                            //     code +=  `sim$add_process(\n`;
                            //     code +=  `    species = "${node.data?.vsSpeciesName}",\n`;
                            //     code +=  `    process_name = "scale_${input_one.vsInputClasses}",\n`;
                            //     code +=  `    process_fun = function() {\n`;
                            //     code +=  `        max_val <- max(self$traits$${input_one.vsInputClasses}, na.rm=TRUE)\n`;
                            //     code +=  `        min_val <- min(self$traits$${input_one.vsInputClasses}, na.rm=TRUE)\n`;
                            //     code +=  `        self$traits$${input_one.vsInputClasses} <-\n`;
                            //     code +=  `            (self$traits$${input_one.vsInputClasses} - min_val) / (max_val - min_val)\n`;
                            //     code +=  `    },\n`;
                            //     code +=  `    execution_priority = ${node.data?.priority || 1}\n`;
                            //     code +=  `)\n`;
                            // }

                            
                        });
                    }   
                }

                code += `\n#### Run the simulation: ####\n`;
                code += `sim$set_time_layer_mapping(rep(1, 10))\n`;
                code += `sim$begin()\n`;
                code = code
                codeOutputElem.innerHTML = code;

                const copyButton = document.querySelector("#code-area .copy button");
                if (copyButton) {
                    copyButton.dataset.code = code;
                }
            }
            editor.on("nodeCreated", updateCodePanel);
            editor.on("nodeRemoved", updateCodePanel);
            editor.on("nodeUnselected", updateCodePanel);
            editor.on("connectionCreated", updateCodePanel);
            editor.on("connectionRemoved", updateCodePanel);
            updateCodePanel();
        }

        const divider = document.getElementById("vs-divider");
        const drawflowArea = document.getElementById("drawflow-area");
        const codeArea = document.getElementById("code-area");
        let isDragging = false;
        let startY = 0;
        let startDrawflowHeight = 0;
        let startCodeHeight = 0;
        divider.addEventListener("mousedown", function (e) {
            e.preventDefault();
            isDragging = true;
            startY = e.clientY;
            startDrawflowHeight = drawflowArea.offsetHeight;
            startCodeHeight = codeArea.offsetHeight;
            document.body.style.cursor = "row-resize";
        });
        window.addEventListener("mousemove", function (e) {
            if (!isDragging) return;
            const dy = e.clientY - startY;
            const vsContainer = document.querySelector(
                ".visual-scripting-container",
            );
            if (vsContainer.classList.contains("fullscreen")) return;
            const totalHeight = vsContainer.offsetHeight;
            let newDrawflowHeight = startDrawflowHeight + dy;
            let newCodeHeight = startCodeHeight - dy;
            // Set min/max heights
            const minDrawflow = 250;
            const minCode = 80;
            if (newDrawflowHeight < minDrawflow) {
                newDrawflowHeight = minDrawflow;
                newCodeHeight =
                    totalHeight - minDrawflow - divider.offsetHeight;
            } else if (newCodeHeight < minCode) {
                newCodeHeight = minCode;
                newDrawflowHeight =
                    totalHeight - minCode - divider.offsetHeight;
            }
            drawflowArea.style.height = newDrawflowHeight + "px";
            codeArea.style.height = newCodeHeight + "px";
        });
        window.addEventListener("mouseup", function () {
            if (isDragging) {
                isDragging = false;
                document.body.style.cursor = "";
            }
        });

        const fullscreenBtn = document.getElementById("vs-fullscreen-toggle");
        const vsContainer = document.querySelector(
            ".visual-scripting-container",
        );
        fullscreenBtn.addEventListener("click", function () {
            vsContainer.classList.toggle("fullscreen");
            if (vsContainer.classList.contains("fullscreen")) {
                fullscreenBtn.textContent = "Exit Fullscreen";
            } else {
                vsContainer.scrollIntoView({ behavior: "instant" });
                fullscreenBtn.textContent = "Fullscreen";
            }
        });
        // Add keybinding to exit fullscreen on Escape
        document.addEventListener("keydown", function (e) {
            if (
                e.key === "Escape" &&
                vsContainer.classList.contains("fullscreen")
            ) {
                vsContainer.classList.remove("fullscreen");
                fullscreenBtn.textContent = "Fullscreen";
                vsContainer.scrollIntoView({ behavior: "instant" });
            }
        });
        const inputToggle = document.getElementById("vs-input-toggle");
        inputToggle.addEventListener("click", function () {
            const inputs = document.querySelectorAll(
                ".drawflow-node .input-container",
            );

            if (inputToggle.textContent === "Hide Inputs") {
                inputToggle.textContent = "Show Inputs";
                inputs.forEach((input) => {
                    input.classList.add("hidden");
                });
            } else {
                inputToggle.textContent = "Hide Inputs";
                inputs.forEach((input) => {
                    input.classList.remove("hidden");
                });
            }
            const allNodes = document.querySelectorAll(
                "#drawflow .drawflow-node",
            );
            allNodes.forEach((node) => {
                editor.updateConnectionNodes(node.id);
            });
        });

    });
</script>
