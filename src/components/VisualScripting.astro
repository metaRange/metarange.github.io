        const zoomInButton = document.getElementById("vs-zoom-in");
        zoomInButton.addEventListener("click", function () {
            editor.zoom_in();
        });
        const zoomOutButton = document.getElementById("vs-zoom-out");
        zoomOutButton.addEventListener("click", function () {
